    "postcss": "^8.5.6",
    "storybook": "^8.5.0",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.21.0",
    "vite": "^6.0.7",
    "vitest": "^4.0.18"
